<div id="content" class="min-vh-100">
    <ol class="breadcrumb">
        <li class="breadcrumb-item active font-weight-bold" aria-current="page">Car</li>
        <li class="breadcrumb-item active font-weight-bold" aria-current="page">List Car</li>
        <li class="breadcrumb-item active" aria-current="page">Add New Car</li>
    </ol>
    <h5>Add New Car</h5>
    <form>
        <div class="form-group row">
            <label for="name" class="col-sm-2 col-form-label">Nama</label>
            <div class="col-sm-4">
                <input type="text" class="form-control" name="name" id="name">
            </div>
        </div>
        <div class="form-group row">
            <label for="Harga" class="col-sm-2 col-form-label">Harga</label>
            <div class="col-sm-4">
                <input type="text" class="form-control" name="harga" id="harga">
            </div>
        </div>
        <div class="form-group row">
            <label for="name" class="col-sm-2 col-form-label">Foto</label>
            <div class="col-sm-4">
                <input type="text" class="form-control" name="foto" id="foto">
            </div>
        </div>
        <div class="form-group row">
            <label for="startRent" class="col-sm-2 col-form-label">Start Rent</label>
            <div class="col-sm-4">
                <input type="text" readonly class="form-control-plaintext" id="startRent" value="-">
            </div>
        </div>
        <div class="form-group row">
            <label for="finishRent" class="col-sm-2 col-form-label">Finish Rent</label>
            <div class="col-sm-4">
                <input type="text" readonly class="form-control-plaintext" id="finishRent" value="-">
            </div>
        </div>
        <div class="form-group row">
            <label for="createdAt" class="col-sm-2 col-form-label">Created at</label>
            <div class="col-sm-4">
                <input type="text" readonly class="form-control-plaintext" id="createdAt" value="-">
            </div>
        </div>
        <div class="form-group row">
            <label for="updatedAt" class="col-sm-2 col-form-label">Updated at</label>
            <div class="col-sm-4">
                <input type="text" readonly class="form-control-plaintext" id="updatedAt" value="-">
            </div>
        </div>
        <div class="" style="margin-top: 20%;">
            <button class="btn" id="cancel-btn">Cancel</button>
            <button type="submit" class="btn" id="save-btn">Save</button>
        </div>
    </form>
</div>